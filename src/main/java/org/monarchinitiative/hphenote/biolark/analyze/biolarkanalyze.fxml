<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.monarchinitiative.hphenote.biolark.analyze.BiolarkAnalyzePresenter">
   <children>
      <SplitPane dividerPositions="0.7" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="400.0" minWidth="700.0" VBox.vgrow="ALWAYS">
        <items>
            <ListView fx:id="chunksListView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label text="HPO terms:">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font></Label>
                  <VBox fx:id="yesTermsVBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" VBox.vgrow="SOMETIMES" />
                  <Separator prefWidth="200.0" />
                  <Label text="&quot;NOT&quot; HPO terms:">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font></Label>
                  <VBox fx:id="notTermsVBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="SOMETIMES" />
               </children>
            </VBox>
        </items>
      </SplitPane>
      <HBox alignment="CENTER" VBox.vgrow="SOMETIMES">
         <children>
            <Button alignment="CENTER" minHeight="30.0" minWidth="120.0" mnemonicParsing="false" onAction="#doneButtonClicked" text="Done">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
